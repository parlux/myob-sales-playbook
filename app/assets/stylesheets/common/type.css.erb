@font-face {
  font-family: 'NeutroMYOB-Regular';
  src: url(<%= asset_path 'NeutroMYOB-Regular.woff2' %>);
}

@font-face {
  font-family: 'NeutroMYOB-Medium';
  src: url(<%= asset_path 'NeutroMYOB-Medium.woff2' %>);
}

@font-face {
  font-family: 'NeutroMYOB-Demi';
  src: url(<%= asset_path 'NeutroMYOB-Demi.woff2' %>);
}

html, body {
  font-family: 'NeutroMYOB-Regular', Arial,sans-serif;
  font-size: 12px;
  line-height: 1.5;
  color: #354555;
}

h1,h2,h3,h4,h5,h6,p { margin: 0; }
